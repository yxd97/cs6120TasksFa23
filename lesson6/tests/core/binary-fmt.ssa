@main(n: int) {
.main_bb0:
  n.1: int = id n;
  call @printBinary n.1;
}

@printBinary(n: int) {
.printBinary_bb0:
  n.1: int = id n;
  zero.1: int = const 0;
  cond.1: bool = eq n.1 zero.1;
  br cond.1 .end .rec;
.rec:
  two.1: int = const 2;
  v0.1: int = call @mod n.1 two.1;
  v1.1: int = div n.1 two.1;
  call @printBinary v1.1;
  print v0.1;
.end:
}

@mod(a0: int, a1: int): int {
.mod_bb0:
  a1.1: int = id a1;
  a0.1: int = id a0;
  v0.1: int = div a0.1 a1.1;
  v1.1: int = mul v0.1 a1.1;
  v2.1: int = sub a0.1 v1.1;
  ret v2.1;
}

